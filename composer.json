{
    "name": "juliosolis/juliosolis/omnipay-bac-fac",
    "description": "First Atlantic Commerce driver for the Omnipay PHP payment processing library\n\n",
    "keywords": [
        "juliosolis",
        "juliosolis/omnipay-bac-fac"
    ],
    "homepage": "https://github.com/juliosolis/omnipay-bac-fac",
    "license": "MIT",
    "authors": [
        {
            "name": "Julio Solis",
            "email": "juliosolai@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "dasprid/enum": "^1.0.3",
        "league/iso3166": "^3.0",
        "omnipay/common": "^3.0",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "omnipay/tests": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\FirstAtlanticCommerce\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\FirstAtlanticCommerce\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
